{
    "name": "data-analysis-chat-participant",
    "publisher": "microsoft",
    "displayName": "Data Analysis Chat Participant",
    "description": "This tool extends the LLM's capabilities by allowing it to run Python code in a sandboxed Python environment (Pyodide) for a wide range of computational tasks and data manipulations that it cannot perform directly.",
    "author": "Microsoft Corporation",
    "homepage": "https://github.com/microsoft/Advanced-Data-Analysis-for-Copilot",
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/Advanced-Data-Analysis-for-Copilot"
    },
    "bugs": {
        "url": "https://github.com/microsoft/Advanced-Data-Analysis-for-Copilot/issues"
    },
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "AI",
        "Chat"
    ],
    "activationEvents": [],
    "enabledApiProposals": [
        "chatVariableResolver",
        "lmTools",
        "chatProvider"
    ],
    "license": "MIT",
    "main": "./out/extension.js",
    "contributes": {
        "chatParticipants": [
            {
                "id": "ada.data",
                "fullName": "Advanced Data Analytics",
                "name": "ada",
                "description": "I can help you with data analytics",
                "isSticky": true
            }
        ],
        "languageModelTools": [
            {
                "id": "ada-data_findFiles",
                "tags": [],
                "displayName": "Find Files",
                "modelDescription": "Search for files in the current workspace",
                "parametersSchema": {
                    "type": "object",
                    "properties": {
                        "pattern": {
                            "type": "string",
                            "description": "Search for files that match this glob pattern"
                        }
                    },
                    "required": [
                        "pattern"
                    ]
                },
                "supportedContentTypes": [
                    "text/plain"
                ]
            },
            {
                "id": "ada-data_runPython",
                "tags": [],
                "displayName": "Run Python",
                "modelDescription": "Execute Python code locally using Pyodide, providing access to Python's extensive functionality. This tool extends the LLM's capabilities by allowing it to run Python code for a wide range of computational tasks and data manipulations that it cannot perform directly. When you know the workspace folder path and the file path, use the relative path to the file when generating code.",
                "parametersSchema": {
                    "type": "object",
                    "properties": {
                        "code": {
                            "type": "string",
                            "description": "The Python code to run"
                        }
                    },
                    "required": [
                        "code"
                    ]
                },
                "supportedContentTypes": [
                    "text/plain",
                    "image/png",
                    "application/vnd.code.notebook.error"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npm run worker && npm run compile-extension",
        "compile-extension": "tsc -p ./",
        "watch-extension": "tsc -p ./ --watch",
        "watch": "concurrently \"npm run watch-extension\" \"npm run worker-watch\"",
        "download-api": "dts main",
        "lint": "eslint src",
        "lint-fix": "eslint --fix src",
        "postinstall": "npm run download-api",
        "worker": "esbuild ./src/execution/src/comlink.worker.ts --bundle --outfile=out/comlink.worker.js --sourcemap --format=esm --platform=node --target=es2018 --define:global=this",
        "worker-watch": "esbuild ./src/execution/src/comlink.worker.ts --bundle --outfile=out/comlink.worker.js --sourcemap --format=esm --platform=node --target=es2018 --define:global=this --watch"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@types/json-schema": "^7.0.15",
        "@types/localforage": "^0.0.34",
        "@types/node": "^20.5.9",
        "@types/node-fetch": "^2.5.7",
        "@types/vscode": "^1.74.0",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^8.8.1",
        "@typescript-eslint/parser": "^8.8.1",
        "@vscode/dts": "^0.4.0",
        "@vscode/prompt-tsx": "^0.2.11-alpha",
        "concurrently": "^9.0.1",
        "esbuild": "^0.21.5",
        "eslint": "^9.12.0",
        "globals": "^15.10.0",
        "run-script-os": "^1.1.6",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.8.1"
    },
    "dependencies": {
        "@jupyterlab/coreutils": "^6.2.2",
        "@jupyterlab/services": "^7.2.2",
        "@jupyterlite/contents": "^0.3.0",
        "@jupyterlite/kernel": "^0.3.0",
        "@lumino/coreutils": "^2.1.2",
        "@vscode/python-extension": "^1.0.5",
        "csv-parser": "^3.0.0",
        "localforage": "^1.10.0",
        "node-fetch": "^2.6.7",
        "pyodide": "0.23.4",
        "strip-ansi": "^7.1.0"
    }
}
